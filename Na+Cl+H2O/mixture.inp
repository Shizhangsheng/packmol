# A mixture of H2O Na Cl

tolerance 2.0
filetype pdb
output mixture.pdb

structure H2O.pdb
  number 1000
  inside box 0. 0. 0. 40. 40. 40.
end structure

structure Na.pdb
  number 400
  inside box 0. 0. 0. 40. 40. 40.
end structure

structure Cl.pdb
  number 400
  inside box 0. 0. 0. 40. 40. 40.
end structure


